        "utility": "cpp",
        "valarray": "cpp",
        "list": "cpp",
        "cstdio": "cpp",
        "*.tcc": "cpp",
        "memory": "cpp",
        "cstring": "cpp",
        "limits": "cpp",
        "ostream": "cpp",
        "cstdint": "cpp",
        "iosfwd": "cpp"
